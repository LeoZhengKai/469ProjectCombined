# Environment variables template for DSPy evaluation framework
# Copy this file to .env and fill in your actual values

# =============================================================================
# API Keys
# =============================================================================

# OpenAI API Key (required for OpenAI models)
OPENAI_API_KEY=your_openai_api_key_here

# Together AI API Key (required for Together AI models)
TOGETHER_API_KEY=your_together_api_key_here

# Anthropic API Key (optional, for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# =============================================================================
# Model Configuration
# =============================================================================

# Default model provider (openai, together, local)
MODEL_PROVIDER=openai

# Default model name
MODEL_NAME=gpt-4o-mini

# Model temperature (0.0 to 1.0)
MODEL_TEMPERATURE=0.7

# Maximum tokens to generate
MODEL_MAX_TOKENS=1000

# Model request timeout in seconds
MODEL_TIMEOUT_SECONDS=30

# =============================================================================
# Evaluation Configuration
# =============================================================================

# Default number of samples for evaluation
EVAL_NUM_SAMPLES=100

# Default dataset split (train, validation, test)
EVAL_SPLIT=test

# Random seed for reproducibility
EVAL_SEED=42

# Quality threshold (0-10)
EVAL_QUALITY_THRESHOLD=8.0

# Factuality threshold (0-10)
EVAL_FACT_THRESHOLD=8.5

# Safety threshold (0-10)
EVAL_SAFETY_THRESHOLD=7.0

# =============================================================================
# RAG Configuration
# =============================================================================

# Enable RAG (true/false)
RAG_ENABLED=false

# Milvus connection URL
RAG_VECTOR_DB_URL=http://localhost:19530

# Vector collection name
RAG_COLLECTION_NAME=embeddings

# Number of retrieved documents
RAG_TOP_K=5

# Embedding model name
RAG_EMBEDDING_MODEL=text-embedding-ada-002

# Similarity threshold for retrieval
RAG_SIMILARITY_THRESHOLD=0.7

# =============================================================================
# MLflow Configuration
# =============================================================================

# Enable MLflow tracking (true/false)
MLFLOW_ENABLED=true

# MLflow tracking URI
MLFLOW_TRACKING_URI=http://localhost:5000

# MLflow experiment name
MLFLOW_EXPERIMENT_NAME=dspy-eval

# MLflow artifact location
MLFLOW_ARTIFACT_LOCATION=mlruns

# =============================================================================
# Logging Configuration
# =============================================================================

# Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Log format (json, text)
LOG_FORMAT=json

# Log file path (null for console only)
LOG_FILE=null

# =============================================================================
# API Configuration
# =============================================================================

# API host
API_HOST=0.0.0.0

# API port
API_PORT=8000

# API key required (true/false)
API_KEY_REQUIRED=false

# Rate limit per minute
API_RATE_LIMIT_PER_MINUTE=100

# Maximum request size in MB
API_MAX_REQUEST_SIZE_MB=10

# =============================================================================
# Performance Configuration
# =============================================================================

# Maximum concurrent evaluations
PERF_MAX_CONCURRENT_EVALUATIONS=5

# Memory limit in MB
PERF_MEMORY_LIMIT_MB=4096

# CPU limit percentage
PERF_CPU_LIMIT_PERCENT=80

# Disk space limit in GB
PERF_DISK_SPACE_LIMIT_GB=10

# =============================================================================
# Security Configuration
# =============================================================================

# Allowed CORS origins (comma-separated)
SECURITY_ALLOWED_ORIGINS=*

# Enable authentication (true/false)
SECURITY_AUTHENTICATION_REQUIRED=false

# Enable authorization (true/false)
SECURITY_AUTHORIZATION_REQUIRED=false

# =============================================================================
# Development Configuration
# =============================================================================

# Enable debug mode (true/false)
DEBUG_MODE=false

# Enable hot reload (true/false)
HOT_RELOAD=true

# Enable profiling (true/false)
PROFILING_ENABLED=false

# Run in test mode (true/false)
TEST_MODE=false

# =============================================================================
# Path Configuration
# =============================================================================

# Experiments directory
EXPERIMENTS_DIR=experiments

# Datasets directory
DATASETS_DIR=datasets

# Configs directory
CONFIGS_DIR=configs

# Artifacts directory
ARTIFACTS_DIR=artifacts

# Logs directory
LOGS_DIR=logs

# Temporary directory
TEMP_DIR=temp

# =============================================================================
# Backup Configuration
# =============================================================================

# Enable automatic backups (true/false)
BACKUP_ENABLED=false

# Backup interval in hours
BACKUP_INTERVAL_HOURS=24

# Backup retention in days
BACKUP_RETENTION_DAYS=30

# Backup location
BACKUP_LOCATION=backups

# =============================================================================
# Monitoring Configuration
# =============================================================================

# Health check interval in seconds
MONITORING_HEALTH_CHECK_INTERVAL=30

# Enable metrics collection (true/false)
MONITORING_METRICS_COLLECTION=true

# Enable alerting (true/false)
MONITORING_ALERTING_ENABLED=false

# Alert webhook URL
MONITORING_ALERT_WEBHOOK_URL=null

# =============================================================================
# Project-Specific Configuration
# =============================================================================

# SharkTank configuration
SHARKTANK_QUALITY_THRESHOLD=8.0
SHARKTANK_FACT_THRESHOLD=8.5
SHARKTANK_MAX_REFINEMENT_ITERATIONS=3

# ANEETA configuration
ANEETA_SAFETY_THRESHOLD=7.0
ANEETA_QUALITY_THRESHOLD=8.0
ANEETA_BIAS_THRESHOLD=8.0
ANEETA_PRIVACY_LEVEL=high
ANEETA_BIAS_DETECTION_ENABLED=true

# =============================================================================
# Local Model Configuration
# =============================================================================

# Local inference server URL
LOCAL_MODEL_BASE_URL=http://localhost:8000

# Quantization level (none, 4bit, 8bit, 16bit)
LOCAL_MODEL_QUANTIZATION=4bit

# Memory usage in GB
LOCAL_MODEL_MEMORY_USAGE_GB=2.0

# Batch size for inference
LOCAL_MODEL_BATCH_SIZE=1

# Maximum sequence length
LOCAL_MODEL_MAX_SEQUENCE_LENGTH=2048

# Use memory-efficient attention (true/false)
LOCAL_MODEL_MEMORY_EFFICIENT_ATTENTION=true

# =============================================================================
# Database Configuration
# =============================================================================

# Database URL (for production use)
DATABASE_URL=sqlite:///experiments/database.db

# Database connection pool size
DATABASE_POOL_SIZE=10

# Database connection timeout
DATABASE_TIMEOUT=30

# =============================================================================
# Cache Configuration
# =============================================================================

# Enable caching (true/false)
CACHE_ENABLED=true

# Cache size
CACHE_SIZE=1000

# Cache TTL in seconds
CACHE_TTL_SECONDS=3600

# Redis URL (if using Redis)
REDIS_URL=redis://localhost:6379/0
